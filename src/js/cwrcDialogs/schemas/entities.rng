            <xs:annotation>
                <xs:appinfo>
                    <interface-field path="entity/person|organization|place/description/existDates/dateSingle/dateType">
                        <help-text xml:lang="en">Captures a date type in relation to its association
                            with the entity described. Prescribed values: person entities: birth,
                            death, flourish; organization and place entities: establishment,
                            dissolution, flourish</help-text>
                        <label xml:lang="en">Type</label>
                        <input>combobox</input>
                    </interface-field>
                    <values type="person">
                        <choice>
                            <value a:en="birth">birth</value>
                            <value a:en="death">death</value>
                            <value a:en="flourish">flourish</value>
                        </choice>
                    </values>
                    <values type="organization">
                        <choice>
                            <value a:en="establishment">establishment</value>
                            <value a:en="dissolution">dissoluton</value>
                            <value a:en="flourish">flourish</value>
                        </choice>
                    </values>
                    <values type="place">
                        <choice>
                            <value a:en="establishment">establishment</value>
                            <value a:en="dissolution">dissoluton</value>
                            <value a:en="flourish">flourish</value>
                        </choice>
                    </values>
                </xs:appinfo>
            </xs:annotation>
            <choice>
                <value a:en="birth">birth</value>
                <value a:en="death">death</value>
                <value a:en="flourish">flourish</value>
                <value a:en="establishment">establishment</value>
                <value a:en="dissolution">dissoluton</value>
            </choice>
        </element>
    </define>

    <define name="note-element">
        <element name="note">
            <a:documentation>A field to record a note about some aspect of the present
